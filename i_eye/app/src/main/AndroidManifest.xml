        </activity>
        <activity android:name=".SplashActivity" />
        <activity android:name=".ReservedMainActivity" />
        <activity android:name=".SearchDestinationActivity" />
        <activity android:name=".SearchFavoritesActivity" />
        <activity android:name=".DirectionsActivity" />
        <activity android:name=".FavoritesActivity" />
